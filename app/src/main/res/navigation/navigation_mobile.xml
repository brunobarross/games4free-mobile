<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_mobile"
    app:startDestination="@id/nav_home">
    <fragment
        android:id="@+id/nav_home"
        android:name="com.altamirobruno.games4free.view.HomeFragment"
        android:label="@string/label_home"
        tools:layout="@layout/fragment_home"
        >
        <action
            android:id="@+id/action_nav_home_to_nav_movie"
            app:destination="@id/action_nav_home_to_nav_movie"
            app:popUpTo="@id/nav_home"
            app:popUpToSaveState="true"
            app:restoreState="true" />
    </fragment>

</navigation>